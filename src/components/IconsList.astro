---
import JavaScript from "./icons/JavaScript.astro";
import Css from "./icons/Css.astro";
import Sass from "./icons/Sass.astro";
import Html5 from "./icons/Html5.astro";
import Wordpress from "./icons/Wordpress.astro";
import Php from "./icons/Php.astro";
import React from "./icons/React.astro";
import Vue from "./icons/Vue.astro";
import Github from "./icons/Github.astro";
import NextJs from "./icons/NextJs.astro";
import Prisma from "./icons/Prisma.astro";
import Tailwind from "./icons/Tailwind.astro";
import Typescript from "./icons/Typescript.astro";
---
<div class="flex flex-row flex-wrap gap-5 items-center justify-start mt-2">
    <div class="size-8 lg:size-14 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <NextJs />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Tailwind />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Github />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Css />
    </div>
    <div class="size-8 lg:size-12 stroke-stone-900 dark:stroke-stone-50 transition-all duration-300">
        <Sass />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Html5 />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Wordpress />
    </div>
    <div class="size-8 lg:size-12 stroke-stone-900 dark:stroke-stone-50 transition-all duration-300">
        <Php />
    </div>
    <div class="size-8 lg:size-14 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <JavaScript />
    </div>
    <div class="size-8 lg:size-14 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Typescript />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <React />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Vue />
    </div>
    <div class="size-8 lg:size-12 fill-stone-900 dark:fill-stone-50 transition-all duration-300">
        <Prisma />
    </div>
</div>